  DEFINES += -DLINA_PLATFORM_WINDOWS -DLINA_BUILD_DLL -DLINA_DEBUG -DLINA_DIST
  INCLUDES += -Ivendor/spdlog/include
  FORCE_INCLUDE +=
  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m64 -g
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CPPFLAGS) -m64 -g -std=c++17
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS +=
  LDDEPS +=
  ALL_LDFLAGS += $(LDFLAGS) -L/usr/lib64 -m64 -shared -Wl,--out-implib="../bin/Debug-windows-x86_64/LinaEngine/LinaEngine.lib"
  LINKCMD = $(CXX) -o "$@" $(OBJECTS) $(RESOURCES) $(ALL_LDFLAGS) $(LIBS)
  define PREBUILDCMDS
  endef
  define PRELINKCMDS
  endef
  define POSTBUILDCMDS
	@echo Running postbuild commands
	IF EXIST ..\bin\Debug-windows-x86_64\LinaEngine\LinaEngine.dll\ (xcopy /Q /E /Y /I ..\bin\Debug-windows-x86_64\LinaEngine\LinaEngine.dll ..\bin\Debug-windows-x86_64\Sandbox > nul) ELSE (xcopy /Q /Y /I ..\bin\Debug-windows-x86_64\LinaEngine\LinaEngine.dll ..\bin\Debug-windows-x86_64\Sandbox > nul)
  endef
